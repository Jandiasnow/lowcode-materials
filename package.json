{
  "name": "lowcode-materials",
  "version": "0.1.0",
  "description": "Materials for low-code engine.",
  "keywords": [
    "low-code",
    "low-code engine",
    "antd",
    "materials"
  ],
  "license": "ISC",
  "scripts": {
    "build": "dumi build",
    "dev": "dumi dev",
    "dev:antd": "concurrently \"pnpm --filter 'antd-*' dev\" \"npm run dev\"",
    "dev:markdown": "concurrently \"pnpm --filter 'react-markdown-*' dev\" \"npm run dev\"",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "**/*.js?(x)": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.ts?(x)": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ],
    "**/*.less": [
      "stylelint --allow-empty-input --syntax less",
      "prettier --write"
    ],
    "**/*.{md,json}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@yunti/shared": "workspace:^",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dumi": "^2.2.14",
    "eslint": "^8.45.0",
    "eslint-config-tenx": "^4.0.0",
    "father": "^4.3.6",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-packagejson": "^2.4.5",
    "react": "^17",
    "react-dom": "^17",
    "stylelint": "^13.3.3",
    "stylelint-config-tenx": "^2.1.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.12.0",
    "pnpm": ">=8.1.0"
  },
  "authors": [
    {
      "name": "Carrotzpc",
      "email": "zhang.pc3@gmail.com"
    },
    {
      "name": "Jandiasnow",
      "email": "88074479@qq.com"
    }
  ]
}
