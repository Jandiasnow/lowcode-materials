{
  "name": "@yuntijs/react-markdown-lowcode-materials",
  "version": "1.0.1",
  "description": "react-markdown lowcode materials",
  "keywords": [
    "markdown",
    "react",
    "lowcode"
  ],
  "license": "MIT",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "files": [
    "dist",
    "compiled"
  ],
  "scripts": {
    "build": "father build",
    "build:deps": "father prebundle",
    "dev": "cross-env NODE_ENV=development father dev",
    "prepublishOnly": "father doctor && npm run build"
  },
  "dependencies": {
    "@alilc/lowcode-types": "^1.2.1",
    "@babel/runtime": "^7.0.0",
    "react-markdown": "^9.0.0",
    "rehype-highlight": "^7.0.0",
    "remark-gfm": "^4.0.0"
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "moment": "latest",
    "react": "16.x || 17.x",
    "react-dom": "16.x || 17.x"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "yunti": {
    "umd": {
      "library": "ReactMarkdownLowcode",
      "entry": [
        "/dist/umd/index.min.js"
      ],
      "externals": {
        "react": ">=16.9.0",
        "lodash": ">=4.17.21"
      }
    },
    "lowCode": {
      "meta": "/dist/umd/meta.min.js",
      "editEntry": [
        "/dist/umd/view.min.js"
      ]
    }
  }
}
